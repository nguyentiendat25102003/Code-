#include <iostream>

using namespace std;
int main(){
      int a,b,i=1 ,  a_giai_thua = 1, b_giai_thua = 1;
      cout << "a = ";
      cin >> a;
      cout << "b = ";
      cin >> b;
    
    for( int i=1; i <=a;i++){
        a_giai_thua *=i;
    }  
cout << "a! = " << a_giai_thua  << endl;
    for( int i=1; i <=b;i++){
         b_giai_thua *=i;
     }
cout << " b!= " << b_giai_thua << endl;
cout << "a!/b! = " << a_giai_thua/b_giai_thua << endl;
}
