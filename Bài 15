#include <iostream>

using namespace std;
int main(){
      int n,giai_thua =1,i = 1;
      cout << " n = " << endl;
      cin >> n;
      if(n - 5 <0){
            cout << "n khong co giai thua" << endl;
      }
     else if(n - 5 ==0)
      {giai_thua = i;
      cout << "b! = " << giai_thua << endl;}
     else{
      for(i =1; i <=n-5 ;i++)
     giai_thua *=i;
      cout << "(n-5)! = " << giai_thua << endl;
  }
}
