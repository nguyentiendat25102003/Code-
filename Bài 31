#include <iostream>
int m,n,i ,j ;
   int arr[100][100] = {};  
   using namespace std;
int main(){
  
  cin >> n >> m; 
  for ( i = 0 ; i < n; i++ )
      for ( j =0 ; j < m; j++)
cin >> arr[i][j];
cout << "Nhap gia tri hang rows va columns cá»§a mang 2 chieu :" << endl;
for ( i = 0 ; i < n; i++ ){
      for ( j =0 ; j < m; j++)
        cout << arr[i][j] <<" ";
cout << endl;
      }
for (i = 0 ; i < n ;i++){
  int hang = 0;
  for (j = 0; j < m; j++)
    hang += arr[i][j];
    cout <<" tong hang  " << i + 1 <<"  : " << hang << endl;
 
}
  for (j = 0; j < m; j++){
    int cot = 0;
  for (i = 0 ; i < n ;i++)
    cot += arr[i][j];
    cout <<" tong cot " << j + 1 <<"  : " << cot << endl;
 
  }
} 
