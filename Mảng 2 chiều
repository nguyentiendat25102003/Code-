#include <iostream>
using namespace std;

int main (){

int arr[3][2];
int arr1[2][3] = {10,30,20,40,50,60};
cout << arr1[1][1] << endl;

int i = 0,j =0;
for (int i = 0; i < 2; i++){
 for (int j = 0; j < 3; j++){
 cout << "arr1[" << i << "][" << j << "] = " << arr1[i][j] << endl;
 
 }

}

    return 0;
}
