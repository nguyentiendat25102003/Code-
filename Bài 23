#include <iostream>
using namespace std;
int main(){
   int i = 2,j = 2,ngoc = 0,n;
   cout <<"n =";
   cin >> n;
   if(n < 2){
       cout << "n khong phai so nguyen to" << endl;
   }
else{
    for(i = 2; i < n;i++){
       for (j = 2; j < n;j++){
        if(n % i ==0){
            ngoc++;
        }
        if(ngoc == 0){
cout <<" n la so nguyen to "<<endl;
}
else {
    cout << " n khong phai la so nguyen to " <<endl;    
     } 
  }
  }
}
}
